<style  >
  header {
    font-size: 1.2rem;
    width: 100dvw;
    background: transparent;
    border-bottom: 2px solid  var(--color-principal);

    & .container {
      max-width: 70dvw;
      margin: auto;
      display: flex;
      align-items: center;
      justify-content: space-between;
      padding:  0.7rem;
    }
    
    & .brand {
      color: var(--texto-muted);
      font-weight: bolder;
      display: flex;
      justify-content: center;
      align-items: center;
      gap:0.5rem;
      user-select: none;
      cursor: pointer;
      text-decoration: none;
      font-family: var(--texto-titulo-general);
      transition: color 0.15s ease-in-out;
  
      & svg {
        width: 1.5rem;
        height: 1.5rem;
        stroke: var(--color-principal);
      }

      & span {
        font-size: 1.3rem;
        font-weight: bold;
        text-transform: uppercase;
        color: white;
      }
    }

    & nav {
      display: flex;
      gap: 1rem;

      & a {
        display: flex;
        flex-direction: row;
        align-items: center;
        gap: 0.3rem;
        width: 100%;
        text-decoration: none;
        color: var(--texto-muted);
        font-weight: 600;
        font-family: var(--texto-titulo-emergente);
        transition: all 0.15s ease-in-out;
        text-wrap: nowrap;

        &:hover {
          color: white;

          & svg {
            filter: grayscale(0%);
          }
        }

        & svg {
          width: 1.5rem;
          height: 1.5rem;
          filter: grayscale(25%);
          transition: all 0.15s ease-in-out;
        }
      }
    }
  }

</style>